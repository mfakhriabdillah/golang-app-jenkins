    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Hello World'
                echo 'Building application...'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello World'
                echo 'Testing application...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello World'
                echo 'Deploy application...'
            }
        }
    }
}
